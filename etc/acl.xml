<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="MagePrakash_OutofStockNotification::config" title="MagePrakash Stock Notification Configuration" sortOrder="50"></resource>
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Catalog::catalog">
					<resource id="MagePrakash_OutofStockNotification::notification" title="MagePrakash Stock Notification Label" translate="title" sortOrder="10" />
			        <resource id="MagePrakash_OutofStockNotification::notificationlist" title="MagePrakash Stock Notification Label Menu" translate="title" sortOrder="10">
                        <resource id="MagePrakash_OutofStockNotification::massdelete" title="MagePrakash Stock Notification Delete" sortOrder="30" />
                    </resource>
				</resource>
			</resource>
        </resources>
    </acl>
</config>